{
  "presets": ["react-app"],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["."],
        "alias": {
          "@src": ["./src"],
          "@ducks": ["./src/ducks"],
          "@images": ["./src/assets/images"],
          "@utils": ["./src/utils"]
        }
      }
    ]
  ]
}